[user]
  email = joshnuss@gmail.com
  name = Josh Nussbaum
[github]
  user = joshnuss
[push]
  default = matching
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  aa = add .
  uncommit = reset --soft HEAD^
[color]
  ui = 1
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[core]
  excludesfile = ~/.gitignore
